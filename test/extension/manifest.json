{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "description": "A simple Chrome extension.",
    "permissions": ["storage", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon1.png",
        "48": "images/icon2.png",
        "128": "images/icon3.png"
      }
    }
  }
  